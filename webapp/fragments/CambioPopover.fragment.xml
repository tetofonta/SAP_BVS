<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Popover
		title="Cambio"
		class="sapUiContentPadding"
		placement="Bottom"
			initialFocus="email">
		<footer>
		</footer>
		<HBox id="cosse">
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout eschifo" header="Battuta brutta"
				subheader="" press="battutabrutta">
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout emah" header="Battuta mediocre"
				subheader="" press="battutabella">
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout ewow" header="Battuta Bella"
				subheader="" press="battutafantastica">
			</GenericTile>
		</HBox>
		
		<List
			items="{
				path: ''
			}">
			<StandardListItem
				type="Active"
				press="changePlayer"
				title="{nome}"
				description="{numero}" />
		</List>
	</Popover>
</core:FragmentDefinition>